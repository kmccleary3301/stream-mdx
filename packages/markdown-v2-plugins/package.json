{
  "name": "@stream-mdx/plugins",
  "version": "0.0.2",
  "description": "Streaming Markdown V2 plugin suite (math, MDX, tables, callouts, footnotes, HTML helpers)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmccleary3301/stream-mdx.git",
    "directory": "packages/markdown-v2-plugins"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./registry": {
      "types": "./dist/plugins/registry.d.ts",
      "import": "./dist/plugins/registry.mjs",
      "require": "./dist/plugins/registry.cjs"
    },
    "./base": {
      "types": "./dist/plugins/base.d.ts",
      "import": "./dist/plugins/base.mjs",
      "require": "./dist/plugins/base.cjs"
    },
    "./callouts": {
      "types": "./dist/plugins/callouts/index.d.ts",
      "import": "./dist/plugins/callouts/index.mjs",
      "require": "./dist/plugins/callouts/index.cjs"
    },
    "./document": {
      "types": "./dist/plugins/document.d.ts",
      "import": "./dist/plugins/document.mjs",
      "require": "./dist/plugins/document.cjs"
    },
    "./footnotes": {
      "types": "./dist/plugins/footnotes/index.d.ts",
      "import": "./dist/plugins/footnotes/index.mjs",
      "require": "./dist/plugins/footnotes/index.cjs"
    },
    "./html": {
      "types": "./dist/plugins/html/index.d.ts",
      "import": "./dist/plugins/html/index.mjs",
      "require": "./dist/plugins/html/index.cjs"
    },
    "./math": {
      "types": "./dist/plugins/math/index.d.ts",
      "import": "./dist/plugins/math/index.mjs",
      "require": "./dist/plugins/math/index.cjs"
    },
    "./math/renderer": {
      "types": "./dist/plugins/math/renderer.d.ts",
      "import": "./dist/plugins/math/renderer.mjs",
      "require": "./dist/plugins/math/renderer.cjs"
    },
    "./mdx": {
      "types": "./dist/plugins/mdx/index.d.ts",
      "import": "./dist/plugins/mdx/index.mjs",
      "require": "./dist/plugins/mdx/index.cjs"
    },
    "./tables": {
      "types": "./dist/plugins/tables/index.d.ts",
      "import": "./dist/plugins/tables/index.mjs",
      "require": "./dist/plugins/tables/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@stream-mdx/core": "0.0.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
